<div class="container px-3">
  <h2 style="display: inline-block;">My roommates</h1>
  <div class="row">
    <% @flat.users.each do |user| %>
      <div class="col-10 col-sm-6 col-md-4 col-lg-3 mx-auto my-2">
        <%= link_to chore_list_index_path(current_user) do %>
          <%= render partial: "user_card", locals: {user: user} %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
